{
    "name": "vue3-component-library",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "pnpm -r dev",
        "build": "pnpm -r build",
        "docs:dev": "pnpm --filter @vue3-component-library/docs docs:dev",
        "docs:build": "pnpm --filter @vue3-component-library/docs docs:build",
        "docs:preview": "pnpm --filter @vue3-component-library/docs docs:preview",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
        "format": "prettier --write .",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@types/node": "^20.11.19",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "@vitejs/plugin-vue": "^5.0.4",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.22.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "typescript": "^5.3.3",
        "vite": "^5.0.0",
        "vitepress": "^1.0.0-rc.40",
        "vue": "^3.3.4"
    },
    "engines": {
        "node": ">=16"
    }
}